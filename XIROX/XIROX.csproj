		<PackageReference Include="MailKit" Version="4.13.0" />
		<PackageReference Include="Microsoft.AspNetCore.ResponseCompression" Version="2.3.0" />
		<PackageReference Include="Microsoft.jQuery.Unobtrusive.Validation" Version="4.0.0" />

		<!-- ASP.NET Core caching (optional but good for performance) -->
		<PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="9.0.9" />
		<PackageReference Include="NetEscapades.AspNetCore.SecurityHeaders" Version="1.2.0" />
		<PackageReference Include="SixLabors.ImageSharp.Web" Version="3.2.0" />
		<PackageReference Include="Westwind.AspNetCore.LiveReload" Version="0.5.2" />
	</ItemGroup>

	<ItemGroup>
	  <EditorConfigFiles Remove="D:\Visual Studio Community\Project\Project\XIROX\XIROX\.editorconfig" />
