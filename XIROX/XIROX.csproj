<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<OutputType>Exe</OutputType>
		<ApplicationIcon>XIROX_ICON (1).ico</ApplicationIcon>
	</PropertyGroup>

	<ItemGroup>
		<!-- jQuery and validation for forms -->
		<PackageReference Include="jQuery" Version="3.7.1" />
		<PackageReference Include="jQuery.Validation" Version="1.21.0" />
		<PackageReference Include="LigerShark.WebOptimizer.Core" Version="3.0.477" />
